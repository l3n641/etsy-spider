{
  "name": "etsy 采集工具",
  "description": "etsy 采集工具",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "notifications"
  ],
  "host_permissions": [
    "https://www.etsy.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "matches": [
        "https://www.etsy.com/*"
      ]
    }
  ],
  "action": {
    "default_popup": "popup/index.html"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "manifest_version": 3
}
